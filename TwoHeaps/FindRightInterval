class Solution:
    def findRightInterval(self, intervals: List[List[int]]) -> List[int]:

        if len(intervals)==1 and intervals[0][0]==intervals[0][1]:
            return [0]
        if len(intervals)<=1:
            return [-1]
        right_intervals=[]
        left_intervals=[]
        res=res = [-1] * len(intervals)

        for i in range(len(intervals)):
            left,right=intervals[i]
            heapq.heappush(right_intervals,(right,i))
            heapq.heappush(left_intervals, (left,i))
        
        while right_intervals:
            r,ia=heapq.heappop(right_intervals)
            while left_intervals:
                l,ir=heapq.heappop(left_intervals)
                
                if l>=r:
                    heapq.heappush(left_intervals,(l,ir))
                    res[ia]=ir
                    break
        return res



        

        

        
